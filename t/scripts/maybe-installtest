#!/bin/sh
if test -n "$FLUX_SCHED_TEST_INSTALLED" -o -n "$FLUX_TEST_INSTALLED_PATH"; then
  export FLUX_EXEC_PATH_PREPEND="${FLUX_SCHED_EXEC_DIR}"
  export FLUX_MODULE_PATH_PREPEND="${FLUX_SCHED_MOD_DIR}"
  export PYTHONPATH="${FLUX_SCHED_PYTHON_SITELIB}${PYTHONPATH:+:${PYTHONPATH}}"
fi
exec flux env $@
